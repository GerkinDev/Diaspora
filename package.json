{
	"name": "diaspora",
	"version": "0.2.0-rc.2",
	"description": "Multi-Layer ORM for Javascript Client+Server",
	"main": "diaspora.js",
	"files": [
		"dist",
		"lib",
		"!.DS_Store"
	],
	"directories": {
		"doc": "doc",
		"lib": "lib",
		"test": "test"
	},
	"scripts": {
		"test": "mocha test/index.js",
		"test-coverage": "nyc --reporter=lcov mocha test/index.js",
		"dist": "grunt dist && grunt documentate",
		"build-tests": "grunt refreshTests",
		"build-then-check": "npm run dist && npm run build-tests && npm run test",
		"preversion": "script/deploy",
		"version": "npm run build-then-check && git add ."
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Nihilivin/Diaspora.git"
	},
	"keywords": [
		"ORM",
		"db",
		"collection",
		"manager",
		"localstorage",
		"webstorage",
		"models",
		"mvc",
		"query",
		"object"
	],
	"author": "Gerkin",
	"license": "GPL-3.0",
	"bugs": {
		"url": "https://github.com/Nihilivin/Diaspora/issues"
	},
	"homepage": "https://github.com/Nihilivin/Diaspora#readme",
	"devDependencies": {
		"@comandeer/babel-plugin-banner": "^1.0.0",
		"babel-core": "^6.26.0",
		"babel-loader": "^7.1.2",
		"babel-preset-env": "^1.6.0",
		"chai": "^4.1.2",
		"chai-as-promised": "^7.1.1",
		"chalk": "^2.1.0",
		"check-types": "^7.3.0",
		"diaspora_doc": "git+https://github.com/GerkinDev/DiasporaDoc.git",
		"docco-husky": "git+https://github.com/GerkinDev/docco-husky.git",
		"eslint-plugin-jsdoc": "^3.1.3",
		"grunt": "^1.0.1",
		"grunt-babel": "^7.0.0",
		"grunt-browserify": "^5.2.0",
		"grunt-changed": "^1.1.1",
		"grunt-contrib-clean": "^1.1.0",
		"grunt-contrib-copy": "^1.0.0",
		"grunt-contrib-uglify": "^3.1.0",
		"grunt-docco-husky": "git+https://github.com/GerkinDev/grunt-docco-husky.git",
		"grunt-jsdoc": "^2.2.0",
		"gruntify-eslint": "^4.0.0",
		"ink-docstrap": "^1.3.0",
		"load-grunt-tasks": "^3.5.2",
		"mocha": "^4.0.1",
		"node-localstorage": "^1.3.0",
		"nyc": "^11.2.1",
		"saucelabs": "^1.4.0",
		"selenium-webdriver": "^3.6.0",
		"stack-trace": "0.0.10"
	},
	"dependencies": {
		"bluebird": "^3.5.1",
		"lodash": "^4.17.4",
		"sequential-event": "^0.2.0",
		"winston": "^3.0.0-rc0"
	}
